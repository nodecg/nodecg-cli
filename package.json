{
  "name": "nodecg-cli",
  "description": "The NodeCG command line interface.",
  "version": "6.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/nodecg/nodecg-cli.git"
  },
  "bugs": {
    "url": "http://github.com/nodecg/nodecg-cli/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/nodecg/nodecg-cli/blob/master/LICENSE"
    }
  ],
  "bin": {
    "nodecg": "bin/nodecg"
  },
  "files": [
    "AUTHORS",
    "LICENSE",
    "README.md",
    "index.js",
    "lib/",
    "commands/",
    "bin/"
  ],
  "engines": {
    "node": ">=6"
  },
  "preferGlobal": true,
  "dependencies": {
    "chalk": "^2.0.0",
    "commander": "^2.19.0",
    "inquirer": "^6.2.1",
    "json-schema-defaults": "^0.3.0",
    "json-schema-to-typescript": "^6.1.0",
    "npm-package-arg": "^4.1.0",
    "request": "^2.51.0",
    "rimraf": "^2.2.8",
    "semver": "^5.1.0"
  },
  "devDependencies": {
    "chai": "^4.0.1",
    "coveralls": "^3.0.0",
    "eslint": "^3.15.0",
    "eslint-config-xo": "^0.18.0",
    "fs-extra": "^7.0.1",
    "istanbul": "^0.4.3",
    "lcov-filter": "0.1.1",
    "mocha": "^5.2.0",
    "sinon": "^4.0.0",
    "standard-version": "^4.4.0",
    "tmp": "0.0.33"
  },
  "scripts": {
    "test": "npm run static && istanbul cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "node node_modules/lcov-filter/index.js ./coverage/lcov.info test tmp | coveralls && rm -rf ./coverage",
    "static": "eslint .",
    "prerelease": "npm t",
    "release": "standard-version",
    "postrelease": "npm publish && git push --follow-tags"
  },
  "license": "MIT"
}
