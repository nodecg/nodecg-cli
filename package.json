{
  "name": "nodecg-cli",
  "version": "6.1.0",
  "description": "The NodeCG command line interface.",
  "bugs": {
    "url": "http://github.com/nodecg/nodecg-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nodecg/nodecg-cli.git"
  },
  "license": "MIT",
  "bin": {
    "nodecg": "dist/bin/nodecg.js"
  },
  "files": [
    "AUTHORS",
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "format": "prettier --write \"**/*.ts\"",
    "static": "run-s static:*",
    "static:eslint": "eslint --cache \"**/*.ts\"",
    "static:prettier": "prettier --check \"**/*.ts\"",
    "test": "jest --colors --coverage"
  },
  "prettier": "@gamesdonequick/prettier-config",
  "dependencies": {
    "chalk": "3.0.0",
    "commander": "^6.1.0",
    "fs-extra": "7.0.1",
    "inquirer": "^7.3.3",
    "json-schema-defaults": "0.3.0",
    "json-schema-to-typescript": "6.1.0",
    "npm-package-arg": "6.1.0",
    "request": "2.88.0",
    "rimraf": "2.6.3",
    "semver": "5.6.0",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@gamesdonequick/prettier-config": "^2.2.0",
    "@types/fs-extra": "5.0.4",
    "@types/hosted-git-info": "2.7.0",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.14",
    "@types/node": "10.12.18",
    "@types/npm-package-arg": "6.1.0",
    "@types/request": "2.48.1",
    "@types/rimraf": "2.0.2",
    "@types/semver": "5.5.0",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-xo": "^0.32.1",
    "eslint-config-xo-typescript": "^0.33.0",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "standard-version": "8.0.1",
    "tmp": "0.2.1",
    "ts-jest": "^26.4.1",
    "type-fest": "^0.17.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=8"
  }
}
